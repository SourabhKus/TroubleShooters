set PATH=%PATH%;C:\OSGeo4W64\apps\Python37\Scripts;C:\Windows\System32\downlevel;

Now add Python scripts in your system PATH
------------------------------------------
set PATH=%PATH%;C:\Python37-32\Scripts;C:\Windows\System32\downlevel;


pyinstaller C:\OSGeo4W64\bin\gdal2tiles.py -F -p "C:\OSGeo4W64\apps\Python37\lib\site-packages" --distpath "C:\Users\sourabh\Desktop\Python\bin"


execute py3_env

pyinstaller C:\OSGeo4W64\bin\gdal2tiles.py -F -p "C:\OSGeo4W64\apps\Python37\lib\site-packages" -p "C:\Python37-32\Lib\site-packages"  --distpath "C:\Users\sourabh\Desktop\Python\bin"

pyinstaller C:\OSGeo4W64\bin\gdal2tiles.py --clean -D -p "C:\OSGeo4W64\apps\Python37\lib\site-packages" -p "C:\OSGeo4W64\apps\Python37\lib" --hidden-import=tkinter --hidden-import=PyQt4 --distpath "C:\Users\sourabh\Desktop\Python\bin"

pyinstaller C:\Users\sourabh\Desktop\Python\Qt.py --clean -F --distpath "C:\Users\sourabh\Desktop\Python\bin"
